<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p" 
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-4.3.xsd"> 
	<bean id="dept" class="cn.mldn.mldnspring.vo.Dept" scope="singleton" lazy-init="true"> 
		<property name="deptno" value="10"/>
		<property name="dname" value="财务部"/>
	</bean>  
	<bean id="message" class="cn.mldn.mldnspring.vo.Message" init-method="createChannel" destroy-method="closeChannel"/>
</beans> 
